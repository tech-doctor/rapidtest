{
	"name": "rapidtest",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"start": "yarn dev & yarn server",
		"preview": "vite preview",
		"scrape": " node src/lib/server/scraper/scraper.js",
		"server": "yarn tsx src/lib/server/api",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"db:start": "sudo docker compose up -d",
		"db:stop": "sudo docker compose down",
		"db:connect": "sudo docker exec -it my-postgres-db psql -U tee-user  -d local",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.2.11",
		"@sveltejs/kit": "^2.19.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/axios": "^0.14.4",
		"@types/cheerio": "^0.22.35",
		"@types/express": "^5.0.0",
		"@types/pg": "^8.11.11",
		"drizzle-kit": "^0.30.2",
		"nodemon": "^3.1.9",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.22.6",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"cheerio": "^1.0.0",
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.40.0",
		"hono": "^4.7.4",
		"pg": "^8.13.3",
		"postgres": "^3.4.5",
		"puppeteer": "^24.4.0"
		
	}
}
